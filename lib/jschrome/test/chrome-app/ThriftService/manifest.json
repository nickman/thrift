{
     "name": "ThriftService",
     "description": "Background Thrift Service",
     "version": "1.0",
     "app": {
          "background": {
               "scripts": [
                    "jquery-2.0.3.js",
                    "async.js",
                    "thrift-chrome-transport.js",
                    "chrome-socket-api-errors.js",
                    "thrift-chrome-protocol.js",
                    "background.js"
     	    ]
	    }
     },
     "icons": {
          "128": "/img/ts_128.png",
          "16": "/img/ts_16.png"
     },
     "permissions": [
          "idle",
          {"socket" : ["tcp-connect:*:*", "udp-bind", "udp-send-to"]}
          
     ],
     "manifest_version": 2
}